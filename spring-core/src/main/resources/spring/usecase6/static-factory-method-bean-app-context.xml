<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
            http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="episodeResult" class="com.dee.web.spring.usecase6.ResultCrawlerStaticFactoryMethod"
		factory-method="createCrawlerData">
        <constructor-arg value="episode" />
	</bean>
	<bean id="seriesResult" class="com.dee.web.spring.usecase6.ResultCrawlerStaticFactoryMethod"
		factory-method="createCrawlerData">
        <constructor-arg value="series" />
	</bean>
</beans>